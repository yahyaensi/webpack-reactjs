{
  "name": "webpack-reactjs",
  "version": "1.0.0",
  "description": "Integration of webpack and reactjs",
  "private": true,
  "scripts": {
    "start": "npm run build",
    "build": "webpack --config webpack.dev.config.js && webpack-dev-server --config webpack.dev.config.js",
    "build:prod": "webpack -p && cp src/index.html dist/index.html",
    "fix-code": "prettier-eslint --write \"src/**/*.{js,jsx}\"",
    "fix-styles": "prettier-stylelint --write \"src/**/*.{css,scss}\""
  },
  "keywords": [
    "reactjs",
    "webpack"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yahyaensi/webpack-reactjs.git"
  },
  "author": "Yahya Romdhane",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/yahyaensi/webpack-reactjs/issues"
  },
  "homepage": "https://github.com/yahyaensi/webpack-reactjs#readme",
  "dependencies": {
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-router-dom": "^5.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.4.5",
    "@babel/plugin-proposal-class-properties": "^7.4.4",
    "@babel/preset-env": "^7.4.5",
    "@babel/preset-react": "^7.0.0",
    "autoprefixer": "^9.5.1",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.6",
    "clean-webpack-plugin": "^2.0.2",
    "css-loader": "^2.1.1",
    "csv-loader": "^3.0.2",
    "eslint": "^5.16.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^4.3.0",
    "eslint-plugin-import": "^2.17.3",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-react": "^7.13.0",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^3.0.1",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "3.0.7",
    "mini-css-extract-plugin": "^0.6.0",
    "node-sass": "^4.12.0",
    "optimize-css-assets-webpack-plugin": "^5.0.1",
    "postcss-cssnext": "^3.1.0",
    "postcss-import": "^12.0.1",
    "postcss-loader": "^3.0.0",
    "postcss-preset-env": "^6.6.0",
    "prettier-eslint": "^8.8.2",
    "prettier-eslint-cli": "^4.7.1",
    "prettier-stylelint": "^0.4.2",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "stylelint": "^10.0.1",
    "stylelint-config-prettier": "^5.2.0",
    "stylelint-prettier": "^1.1.0",
    "sugarss": "^2.0.0",
    "terser-webpack-plugin": "^1.2.4",
    "uglifyjs-webpack-plugin": "^2.1.3",
    "webpack": "^4.31.0",
    "webpack-bundle-analyzer": "^3.3.2",
    "webpack-cli": "^3.3.2",
    "webpack-dev-server": "^3.4.1",
    "webpack-merge": "^4.2.1",
    "xml-loader": "^1.2.1"
  }
}
